{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxyExport": {
    "ServiceName": "test-service",
    "InitialRevision": 1,
    "EnablePeriodicRepublish": true,
    "RepublishInterval": "00:00:05",
    "Routes": {
      "test-route": {
        "RouteId": "test-route",
        "ClusterId": "test-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "test-cluster": {
        "ClusterId": "test-cluster",
        "LoadBalancingPolicy": "RoundRobin"
      }
    }
  }
}